# Reset to default
RESET_HW


# LCD message after load
LCD "%C@(1:1:1)AINSER HQ Test"


# AINSER64 hardware
# only first 16 inputs are used to send Pitchbend events

# AINSER n=  1   enabled=1  muxed=1  cs=0  resolution=12bit
# use 10bit instead of 12bit for reduced (resp. on good HW: no!) jitter
AINSER n=  1   enabled=1  muxed=1  cs=0  resolution=10bit


# EVENTs
EVENT_AINSER id=  1  type=PitchBend  chn= 1  range=0:16383 offset=  0  ports=1000100000001000  lcd_pos=1:1:1  label="^std_aser"
